<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.franc.mapper.AcntMapper">

    <select id="findById" parameterType="Long" resultType="AcntVO">
        /** AcntMapper.findById */
        SELECT A.*
        FROM TB_ACNT A
        WHERE A.ACNT_ID = #{acntId}
    </select>

</mapper>